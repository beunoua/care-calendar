<tr class="week">
    <td class="{{master.get_css_class_week_number()}}">{{week_id}}</td>
    <td class="features">
        <table class="features">
        {% for day in week %}
            <tr class="{{master.get_css_class_date(day)}}">
            {% for feat in master.features %}
                <td {{ feat.format_attrs(day) }}>{{ feat.format_text(day).strip() }}</td>
            {% endfor %}
            </tr>
        {% endfor %}
        </table>
    </td>
</tr>
